<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.LoginFormController">

    <children>
        <TextField fx:id="name" layoutX="61.0" layoutY="258.0" prefHeight="25.0" prefWidth="391.0" promptText="Enter The User Name" />
        <Button layoutX="461.0" layoutY="258.0" mnemonicParsing="false" onAction="#onClick" prefHeight="25.0" prefWidth="57.0" style="-fx-background-color: #6fe5f2;" text="Login" />
      <ImageView fitHeight="85.0" fitWidth="79.0" layoutX="261.0" layoutY="151.0" pickOnBounds="true" preserveRatio="true">
